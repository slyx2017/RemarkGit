@{
    ViewBag.Title = "自动生成编号";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script>
    function btn_getcode() {
        $.ajax({
            type: "get",
            url: "/Home/GetAutoCode",
            success: function (result) {
                $("#codename").textbox("setValue",result);
            }
        });
    }
    
</script>

<div class="panel panel-default" style="margin-top:55px;">
    <div class="panel-heading">
        <h3 class="panel-title">自动生成编号</h3>
    </div>
    <div class="panel-body">
        <div id="tb" style="padding:5px;height:auto">
            <div>
                编号<input id="codename" name="codename" class="easyui-textbox" style="width:200px;">
                <a onclick="btn_getcode()" href="#" class="easyui-linkbutton" iconCls="icon-search" style="float:right">生成</a>
            </div>
        </div>
    </div>
</div>

